var div_box = $("#<%= @div_id %>");
// if (div_box[0].id == "facts_demographics")

div_box.find("ul>li.placeholderText").hide();
if ( <%= @update %> == true ){
  var behAttrContent = div_box.find("ul>li#<%= @index %>").html();
  div_box.find("ul>li#<%= @index %>").replaceWith("<%= escape_javascript(render partial: 'inputBehaviour', locals: {persona: @persona, index: @index, div_id: @div_id, update: @update} ) %>");
} else {
  var behAttrContent = ""
  div_box.find("ul").append("<%= escape_javascript(render partial: 'inputBehaviour', locals: {persona: @persona, index: @lastIndex, div_id: @div_id, update: @update}) %>");
}
stopClick = false;
